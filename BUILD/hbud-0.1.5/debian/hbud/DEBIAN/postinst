#!/bin/bash +e

ln -s /usr/share/hbud/HBud.py /usr/bin/hbud || echo "No need for it"

# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p hbud /usr/share/hbud -V 3.4-
fi
if which pypy3compile >/dev/null 2>&1; then
	pypy3compile -p hbud /usr/share/hbud -V 3.4- || true
fi

# End automatically added section
